class MyList<T>
{
    private T[] list;
    private int count;

    public MyList()
    {
        list = new T[0];
        count = 0;
    }

    public void Add(T item)
    {
        Array.Resize(ref list, count + 1);
        list[count] = item;
        count++;
    }

    public T this[int index]
    {
        get { return list[index]; }
    }

    public int Count
    {
        get { return count; }
    }
}

